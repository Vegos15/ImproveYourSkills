<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:layout_height="600dp"
    android:background="@drawable/color_white"
    tools:context="com.example.lord.Fragment_1">

    <EditText
        android:id="@+id/editText"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="-16dp"
        android:background="@drawable/shape_form_for_registration"
        android:ems="10"
        android:hint="@string/project_name"
        android:inputType="textPersonName"
        android:padding="7dp"
        android:autofillHints="" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="216dp"
        android:padding="10dp"
        android:text="@string/create_n_your_n_notebook"
        android:textColor="#393939"
        android:textSize="50sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="200dp"
        android:background="#393939"
         />
    <TextView
        android:id="@+id/textView4"
        android:layout_width="3dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginTop="-70dp"
        android:background="#393939"


        />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="3dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginTop="39dp"
        android:background="#393939"
        />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="3dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginTop="125dp"
        android:background="#393939" />

    <ImageButton
        android:id="@+id/f1_btn_image"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/buttonAdd"
        android:layout_gravity="center"
        android:layout_marginTop="80dp"
        android:contentDescription="@string/todo" />


</FrameLayout>